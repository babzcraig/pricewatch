{
  "compilerOptions": {
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es6",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist"
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}
